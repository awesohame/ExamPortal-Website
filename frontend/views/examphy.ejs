<main class="quiz_sect_wrap">
    <div class="sect_container" id="quiz">
        <div class="quiz_sect">
            <h2 id="question">Question</h2>
            <ul id="ul_container">
                <li class="input_options">
                    <input type="radio" name="Answer" id="a" class="choices">
                    <label for="a" id="a_data">Choice_1</label>
                </li>
                <li class="input_options">
                    <input type="radio" name="Answer" id="b" class="choices">
                    <label for="b" id="b_data">Choice_2</label>
                </li>
                <li class="input_options">
                    <input type="radio" name="Answer" id="c" class="choices">
                    <label for="c" id="c_data">Choice_3</label>
                </li>
                <li class="input_options">
                    <input type="radio" name="Answer" id="d" class="choices">
                    <label for="d" id="d_data">Choice_4</label>
                </li>

            </ul>

        </div>
        <div>
            <button type="submit" id="myBtn" class="next_btn">Submit</button>
        </div>

    </div>

    <div class="questions">
        <p class="questionstext">Questions:</p>
        <div class="questionscontainer">
            <div class="ques">1</div>
            <div class="ques">2</div>
            <div class="ques">3</div>
            <div class="ques">4</div>
            <div class="ques">5</div>
            <div class="ques">6</div>
            <div class="ques">7</div>
            <div class="ques">8</div>
            <div class="ques">9</div>
            <div class="ques">10</div>
            <div class="ques">o</div>
            <div class="ques">o</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
            <div class="ques">0</div>
        </div>
    </div>
    <script src="./quizApp.js"></script>
</main>
